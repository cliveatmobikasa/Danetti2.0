    {%- if request.path == '/collections/vendors' and collection.all_products_count == 0 -%}
      <meta name="robots" content="noindex,nofollow">
    {%- elsif template.name == 'collection' -%}
      {%- liquid
        assign active_filters = collection.filters | map: "active_values" | compact
        assign is_sorted = collection.sort_by
      -%}
      {%- if collection.all_products_count < 4 -%}
        <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" />
        <meta name="robots" content="noindex, follow" />
      {%-elsif collection.metafields.nosto != blank -%}
        {%- if current_page != 1 -%}
          <link rel="canonical" href="{{shop.url}}/collections/{{collection.metafields.nosto.sourceHandle}}?page={{ current_page }}">
          <meta name="robots" content="noindex, follow" />
        {%- else -%}
          <link rel="canonical" href="{{shop.url}}/collections/{{collection.metafields.nosto.sourceHandle}}">
          {%- if active_filters.size > 0 or is_sorted != blank -%}
            <meta name="robots" content="noindex, follow" />
          {%- endif -%}
        {%- endif -%}
      {%- elsif current_page != 1 -%}
        <link rel="canonical" href="{{ shop.url }}{{ collection.url }}?page={{ current_page }}" />
        <meta name="robots" content="noindex, follow" />
      {%- else -%}
        <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" />
        {%- if active_filters.size > 0 or is_sorted != blank -%}
          <meta name="robots" content="noindex, follow" />
        {%- endif -%}
      {%- endif %}
    {%- elsif template == 'cart' -%}
      <meta name="robots" content="noindex, follow" />
    {%- elsif template.name == 'product' -%}
      {%- liquid
        assign is_delisted = false
        if product.tags contains "delisted" or template contains 'delisted'
          assign is_delisted = true
        endif
      -%}
      {%- if is_delisted -%}
        <meta name="robots" content="noindex, follow" {{is_delisted}}/>
      {%- endif -%}
      {% if product.metafields.seo.canonical_url != blank %}
        
        {% if product.metafields.seo.canonical_url != product.handle %}
          
        <meta name="robots" content="noindex, follow" {{product.handle}}/>
        {% endif %}
        <link rel="canonical" href="{{ shop.url }}/products/{{ product.metafields.seo.canonical_url }}" />
      {% endif %}
    {%- else -%}
      <link rel="canonical" href="{{ canonical_url }}" />
    {%- endif -%}

    
