  {%- liquid
    assign siteClasses = 'lyt-Site_Header-solid'

    if template == 'index' or template == 'collection' or template == 'blog' or template == 'article'
      assign siteClasses = 'lyt-Site_Header-clear'

      if template == 'index' or settings.dark_mode == true
        assign siteClasses = siteClasses | append: ' lyt-Site_Header-whiteContent'
      elsif template == 'collection'
        assign collection_for_metafields = collection

        if collection.metafields.nosto.sourceHandle != blank
          assign collection_for_metafields = collections[collection.metafields.nosto.sourceHandle]
        endif

        assign collection_style_lower = collection_for_metafields.metafields.collection_banner.text_colour | downcase

        if collection_style_lower == 'white'
          assign siteClasses = siteClasses | append: ' lyt-Site_Header-whiteContent'
        endif
      elsif template == 'blog'
        assign blog_style_lower = blog.metafields.header.nav_colour | downcase

        if blog_style_lower == 'white'
          assign siteClasses = siteClasses | append: ' lyt-Site_Header-whiteContent'
        endif
      elsif template == 'article'
        assign article_style_lower = article.metafields.header.text_colour | downcase

        if article_style_lower == 'white'
          assign siteClasses = siteClasses | append: ' lyt-Site_Header-whiteContent'
        endif
      endif

    elsif template contains 'page.landing-page'
      if page.metafields.main_nav.background == 'Clear'
        assign siteClasses = 'lyt-Site_Header-clear'

        assign page_style_lower = page.metafields.main_nav.text_colour | downcase

        if page_style_lower == 'white'
          assign siteClasses = siteClasses | append: ' lyt-Site_Header-whiteContent'
        endif
      endif
    endif

    if settings.dark_mode == true
      assign siteClasses = siteClasses | append: ' lyt-Site-dark'
    endif
  -%}

  <body class="gradient{% if settings.animations_hover_elements != 'none' %} animate--hover-{{ settings.animations_hover_elements }}{% endif %} lyt-Site {{ siteClasses }} template-{{ template.name | handle }}">
    
    <script async data-environment="production" src="https://js.klarna.com/web-sdk/v1/klarna.js" data-client-id="1b46487a-ca6f-505f-8987-3c75f00da5ac"></script>

    {%- if settings.enable_gtm_main == true -%}
      {%- render 'google-tag-manager-body' -%}
    {%- endif -%}

    {%- if settings.cart_type == 'drawer' -%}
      {%- render 'cart-drawer' -%}
    {%- endif -%}

    {% sections 'header-group' %}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% sections 'footer-group' %}
    {% render 'drawers' %}
    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
    </ul>

    <script>
      window.shopUrl = '{{ request.origin }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        cart_url: '{{ routes.cart_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}',
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`,
      };

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
        unavailable_with_option: `{{ 'products.product.value_unavailable' | t: option_value: '[value]' }}`,
      };

      window.quickOrderListStrings = {
        itemsAdded: `{{ 'sections.quick_order_list.items_added.other' | t: quantity: '[quantity]' }}`,
        itemAdded: `{{ 'sections.quick_order_list.items_added.one' | t: quantity: '[quantity]' }}`,
        itemsRemoved: `{{ 'sections.quick_order_list.items_removed.other' | t: quantity: '[quantity]' }}`,
        itemRemoved: `{{ 'sections.quick_order_list.items_removed.one' | t: quantity: '[quantity]' }}`,
        viewCart: `{{- 'sections.quick_order_list.view_cart' | t -}}`,
        each: `{{- 'sections.quick_order_list.each' | t: money: '[money]' }}`,
      };

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
        recipientFormExpanded: `{{ 'recipient.form.expanded' | t }}`,
        recipientFormCollapsed: `{{ 'recipient.form.collapsed' | t }}`,
      };
    </script>
    
    {%- comment -%}Wishlist Plus App{%- endcomment -%}
    {%- if settings.enable_wishlist -%}
      {%- render 'swymSnippet' -%}
    {%- endif -%}

    {%- section 'welcome-modal' -%}

    {%- comment -%} Nosto Start {%- endcomment -%}
    {%- render 'nosto-tagging' -%}
    {%- comment -%} Nosto End {%- endcomment -%}

    <script
      async
      type="text/javascript"
      src="https://static.klaviyo.com/onsite/js/klaviyo.js?company_id=JkKgYN"></script>

    {{ 'klevu-user-customization-159724343438812609.js' | asset_url | script_tag }}
    {{ 'header.js' | asset_url | script_tag }}
    {{ 'mobi-constants.js' | asset_url | script_tag }}
    {{ 'mobi-modules.js' | asset_url | script_tag }}
    {{ 'mobi-app.js' | asset_url | script_tag }}

    {% include 'smile-initializer' %}
  </body>
</html>
